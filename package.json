{
  "name": "qor-direct-sdk",
  "version": "3.0.32",
  "description": "Javascript SDK for Qorcommerce.io payment gateway.  See https://docs.qorcommerce.io/ for more information.",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "github": "https://github.com/QorLabs/qor-direct-js-sdk.git",
  "bugs": "https://github.com/QorLabs/qor-direct-js-sdk/issues",
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "docs": "typedoc --options typedoc.json",
    "test": "jest --coverage"
  },
  "keywords": [
    "Qor",
    "QorPay",
    "SDK",
    "JS",
    "TypeScript",
    "NodeJS",
    "JavaScript",
    "Payment Gateway",
    "Credit Card",
    "ACH",
    "Visa",
    "MasterCard",
    "Diners",
    "JCB",
    "Discover"
  ],
  "author": "Russ Palermo <palermo777@gmail.com>",
  "license": "ISC",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/uuid": "^9.0.4",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "isomorphic-unfetch": "^4.0.2"
  }
}
